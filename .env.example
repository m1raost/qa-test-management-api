# Copy this file to .env and fill in real values before running.

# Database — swap for PostgreSQL in production:
# DATABASE_URL=postgresql+psycopg2://user:password@localhost:5432/qa_db
DATABASE_URL=sqlite:///./qa_test_management.db

# JWT — generate with: python -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=change-me-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60
